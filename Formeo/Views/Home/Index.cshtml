        <div class="navigation">
            <div class="row">
                <div class="sidebar">
                    @Html.Partial("Partial/_navigation")
                </div>
                <div ng-show="isMenuTypeIs('0')" class="sidebar-menu-content">
                    <div class="row" ng-show="isActiveMainMenu('/Clients')">
                        @Html.Partial("Partial/Sections/_clients")
                    </div>
                    <div class="row" ng-show="isActiveMainMenu('/Storage')">
                        @Html.Partial("Partial/Sections/_storage")
                    </div>
                </div>

                <!-- TEMP  -->
                <div ng-show="isMenuTypeIs('1')" class="sidebar-menu-content">
                    <div class="row" ng-show="isActiveMainMenu('/Dashboard')">
                        @Html.Partial("Partial/Sections/_dashboard")
                    </div>
                    <div class="row" ng-show="isActiveMainMenu('/Storage')">
                        @Html.Partial("Partial/Sections/_storage")
                    </div>
                    <div class="row" ng-show="isActiveMainMenu('/Orders')">
                        @Html.Partial("Partial/Sections/_orders")
                    </div>
                </div>
                <!-- TEMP  -->
                <!-- TEMP  -->
