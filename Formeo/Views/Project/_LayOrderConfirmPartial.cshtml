
<div class="modal-content">
    <div class="modal-header">
        <h4 class="modal-title">Lay Order</h4>
    </div>
    <div class="modal-body">
        <p>You're about to order following</p>
        <div class="panel panel-default">
            <div class="panel-body">
                <div class="row head">
                    <div class="col-md-3">Art.No.</div>
                    <div class="col-md-3">Product Name</div>
                    <div class="col-md-3">Quantity</div>
                </div>


                <div ng-repeat="printObject in printObjectsInfoModal">
                    <div class="row data-row">
                        <div class="col-md-3">{{printObject.ArtNo}}</div>
                        <div class="col-md-3">{{printObject.Name}}</div>
                        <div class="col-md-3">
                                {{printObject.Quantity}}
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <p>Project Info</p>

        <span ng-bind="orderName"></span>
        <span ng-bind="artNo" ></span>

        <p>Delivery info</p>
        <span ng-bind="deliveryInfo.Surname" ></span>

        <span ng-bind="deliveryInfo.LastName" ></span>

        <span ng-bind="deliveryInfo.Address" ></span>

        <span ng-bind="deliveryInfo.Postal" ></span>

        <span ng-bind="deliveryInfo.City" ></span>

        <span ng-bind="deliveryInfo.Country" ></span>

    </div>
    <div class="modal-footer">
        <button type="button" class="btn btn-default" ng-click="CreateOrder()">Lay order</button>
        <button type="button" class="btn btn-default" ng-click="cancel()">Cancel</button>
    </div>
</div>


